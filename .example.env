# Dockenv Compose 配置信息

# Pre Defined Config
NAME=dockenv
TIMEZONE=Asia/Shanghai
DEFAULT_USER=dockenv
DEFAULT_PASSWD=dockpass
DEFAULT_DB=dockenv
WORKSPACES=~/Code
DOCKER_REGISTRY_NAMESPACE="dockenv"

################################### Docker Registry #######################################

